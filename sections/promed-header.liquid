<!-- TOP NAV BAR -->
<div class="row hidden-sm hidden-xs">

    <div class="pro-top-nav col-lg-7 col-md-7">
        {% if section.settings.show_phone != blank %}
            <i class="fa fa-phone"></i> {{ settings.company_support_phone }}
        {% endif %}

        {% if section.settings.show_email != blank %}
            | <i class="fa fa-envelope"></i> <a href="mailto:{{ settings.company_support_email }}">{{ settings.company_support_email }}</a>
        {% endif %}
    </div>

    <div class="pro-top-nav col-lg-5 col-md-5">
        <span class="pull-right">{% include 'mega-menu' %}</span>
    </div>

</div>

<!-- MID MENU AND CART TOOLS -->
<div class="row">
    <span class="nav-toggle-button-container hidden-xl hidden-lg hidden-md">{% include 'site-nav' %}</span>

    <div class="pro-mid-nav col-lg-8 hidden-sm hidden-xs">
        {% include 'site-nav' %}
    </div>

    <div class="pro-mid-nav col-lg-4 col-sm-12 col-xs-12" style="text-align: right;">

        <span class="pull-left hidden-xl hidden-lg hidden-md">{% include 'pro-header-cart' %}</span>

        {% if shop.customer_accounts_enabled %}
            {% if customer %}

                <a class="btn btn-xs btn-default" href="/account" title="{{ 'layout.customer.account' | t }}">
                    <i class="icon-user icons" aria-hidden="true"></i> <span class="hidden-sm hidden-xs">{{ 'layout.customer.account' | t }}</span>
                </a>

            {% else %}

                <a class="btn btn-xs btn-default" href="/account/login" title="{{ 'layout.customer.log_in' | t }}">
                    <i class="icon-login icons" aria-hidden="true"></i> <span class="hidden-sm hidden-xs">Login</span>
                </a>

                <a class="btn btn-xs btn-default" href="/account/register" title="{{ 'layout.customer.create_account' | t }}">
                    <i class="icon-user icons" aria-hidden="true"></i> <span class="hidden-sm hidden-xs">Register</span>
                </a>

            {% endif %}
        {% endif %}

        {% if section.settings.display_wishlist %}

            <a class="btn btn-xs btn-default" href="/pages/wishlist" title="{{ 'products.wishlist.wishlist_name' | t }}">
                <i class="icon-heart icons" aria-hidden="true"></i> <span class="hidden-sm hidden-xs">{{ 'products.wishlist.wishlist_name' | t }}</span>
            </a>

        {% endif %}

        {% if customer %}
            <a class="btn btn-xs btn-default" href="/account/logout" title="{{ 'layout.customer.log_out' | t }}" data-value="value 1">
                <i class="fa fa-power-off"></i>
            </a>
        {% endif %}

    </div>
</div>

<!-- LOWER NAV BAR -->
<div class="row hidden-md hidden-sm hidden-xs">
    <div class="pro-lower-nav col-lg-8" style="">
        {%  if section.settings.display_megamenu %}
            {% include 'site-nav-mega' %}
        {%  endif %}
    </div>
    <div class="pro-lower-nav col-lg-4 col-sm-8">
        <span>{% include 'pro-header-cart' %}</span>
        <span class="hidden-sm hidden-xs">{% include 'social-links-type-6' %}</span>
    </div>
</div>

